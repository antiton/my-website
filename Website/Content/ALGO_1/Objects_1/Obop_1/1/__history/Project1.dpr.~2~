program Project1;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;
  Var
  a,b,c:integer;
begin
Writeln ('Введите одно двузначное число:');
Readln(a);
if a>9 then
begin
b:=sqr(a);
c:= 4*(((a div(10))*(a div(10))*(a div(10)))+((a mod(10))*(a mod(10))*(a mod(10))));
if b=c then
writeln('Ответ положительный')
else
Writeln('Ответ отрицательный');
end
else
writeln('Это не двузначное число!!!');
readln;
end.
