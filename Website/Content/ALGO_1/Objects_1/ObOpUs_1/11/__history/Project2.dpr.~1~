program Project2;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var n,e : integer;

begin
   write('введите число n=');
   readln(n);
   if n mod 2 = 0 then write('четное') else write ('нечетное');
   readln;
   e := n mod 10;
   if e = 7 then write('заканчивается на 7') else write ('не заканчивается на 7');
   readln;
end.
