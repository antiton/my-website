readln(v1);//ввод с клавиатуры
if (m/v) > (m1/v1) then
    Writeln('ѕлотность первого тела выше второго')//вывод на экран
else
    Writeln('ѕлотность второго тела выше первого');//вывод на экран
readln;
end.         //конец
