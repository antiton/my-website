program Project2;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var
m,m1:integer;
v,v1:integer;   //переменные
begin           //начало
Writeln('¬ведите массу тела: ');//вывод на экран
