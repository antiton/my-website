program Project2;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;
  Const Pi=3.14;

var
Crcl:integer;
Squr:integer;
begin
Writeln('¬ведите длинну стороны квадрата: ');
readln(Squr);
Writeln('¬ведите радиус круга: ');
readln(Crcl);
if (Crcl*Pi*Crcl) < (Squr*Squr) then
    Writeln('ѕлощадь квадрата больше чем площадь круга')
else
    Writeln('ѕлощадь квадрата меньше чем площадь круга');
readln;
end.
