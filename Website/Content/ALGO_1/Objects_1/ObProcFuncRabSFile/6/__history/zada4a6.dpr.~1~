program zada4a6;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

const
  Dir = 'C:\Users\User\Desktop\сайт\ѕз7\з6\File.txt';
  n = 5;
var
  FileCh: file of integer;
  Chislo: integer;
  i: byte;
begin
  assign(FileCh, Dir);
  rewrite(FileCh);
  write('„исло: ');
  readln(Chislo);
  for i:=1 to n do
  begin
    write(FileCh, Chislo);
    write(Chislo, ' ');
  end;
  close(FileCh);
end.
